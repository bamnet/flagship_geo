<!DOCTYPE html>
<html>
<head>
  <title>Geo</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "http://maps.google.com/maps/api/js?sensor=false" %>
  <%= csrf_meta_tag %>
  <% if content_for?(:js_ready) %>
    <script type="text/javascript">
      $(document).ready(function() {
        <%= yield(:js_ready) %>
      });
    </script>
  <% end %>
  <% if content_for?(:js) %>
    <script type="text/javascript">
      <%= yield(:js) %>
    </script>
  <% end %>
</head>
<body>

<%= yield %>

</body>
</html>
